@model IEnumerable<EEBank.Models.UserInf>

@{
    ViewBag.Title = "Создать пользователя";
}
<style>
    
        body {
            background:url('http://ultradesktop.us/wallpaper_3840x2160/textures_background_gray_yellow_blue_ultra_3840x2160_hd-wallpaper-243053.jpg') 20% 20%;
        }
    
</style>
<h2>Создать пользователя</h2>

<p>
    @Html.ActionLink("Добавить пользователя", "Add_User","Account",null, new{ @class = "compItem" }) 
</p>

<table class="table">
    <tr>
        <th>
            ФИО
        </th>
        <th>
            Email
        </th>
        <th>
            Пароль
        </th>
        <th>
            Телефон
        </th>
        <th>
            Номер аккаунта
        </th>
        <th>
            ЭЦП
        </th>
        <th>
            Открытый ключ
        </th>
        <th>
            Адрес
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccountNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ECP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OpenKey)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Adress)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "Edit", new { id=item.UserInfID }) |
            @Html.ActionLink("Подробнее", "Details", new { id=item.UserInfID }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.UserInfID })
        </td>
    </tr>
}

</table>
